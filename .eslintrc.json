{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-trailing-spaces": "error",
    "space-before-function-paren": "error",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ],
    "semi": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2
    ],
    "no-unused-vars": "error",
    "quotes": ["error", "single"]
  }
}